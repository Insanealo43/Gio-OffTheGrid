# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Gio-OffTheGrid' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Gio-OffTheGrid
  pod 'Alamofire', '~> 4.0'     # Networking
  pod 'MBProgressHUD'            # Loader
  pod 'SDWebImage'               # Image Caching
  pod 'DataCache'                # Local Caching
  pod 'Sync', '~> 2'             # Core Data
  pod 'DATAStack', '~> 6'       # Core Data

  target 'Gio-OffTheGridTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Gio-OffTheGridUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
